{
  "name": "structure-ts",
  "version": "0.5.0",
  "description": "A high-level web framework for TypeScript, inspired by Django, designed for rapid development and clean code.",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "bin": {
    "st": "bin/index.js",
    "structure-ts": "bin/index.js"
  },
  "scripts": {
    "build": "npm run build:src && npm run build:cli",
    "build:src": "tsc -b",
    "build:cli": "tsc -p tsconfig.cli.json",
    "check": "biome check",
    "check:fix": "biome check --write",
    "commitlint": "commitlint --edit",
    "dev": "cd local && tsx --watch src/main.ts",
    "dev:cli": "tsx --tsconfig tsconfig.cli.json cli/main.ts",
    "prepare": "husky",
    "test": "tsx --test tests/**/**/*.test.ts tests/**/*.test.ts",
    "test:w": "tsx --watch --test tests/**/**/*.test.ts tests/**/*.test.ts"
  },
  "files": [
    "lib",
    "bin",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "better-sqlite3": "12.*",
    "mongodb": "7.*",
    "mysql2": "3.*",
    "pg": "8.*"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "biome check --write"
    ],
    "*": [
      "biome check --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulroy0322/structure-ts.git"
  },
  "author": "Rahul Roy <rahulroyapd80@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rahulroy0322/structure-ts/issues"
  },
  "homepage": "https://github.com/rahulroy0322/structure-ts#readme",
  "engines": {
    "node": ">= 16.14"
  },
  "keywords": [
    "structure-ts",
    "framework",
    "django",
    "opinionated",
    "structure",
    "web",
    "http",
    "rest",
    "restful",
    "router",
    "app"
  ],
  "dependencies": {
    "chalk": "^5.6.2",
    "ejs": "^3.1.10",
    "joi": "^17.13.3",
    "uid": "^2.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.13.13",
    "@types/pg": "^8.15.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "mongodb": "^7.0.0",
    "mysql2": "^3.15.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
